{
  "name": "or-jobsity-challenge",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.1.1",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "autoprefixer": "^9.8.6",
    "axios": "^0.21.1",
    "core-js": "^3.9.1",
    "framer-motion": "^3.10.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "postcss": "^7.0.35",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1",
    "react": "^17.0.1",
    "react-app-polyfill": "^2.0.0",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.1",
    "react-modal": "^3.12.1",
    "react-redux": "^7.2.2",
    "react-scripts": "4.0.3",
    "react-select": "^4.2.1",
    "react-time-picker": "^4.1.2",
    "redux": "^4.0.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-thunk": "^2.3.0",
    "styled-components": "^5.2.1",
    "tailwind-scrollbar": "^1.0.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.3",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test --silent",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier",
      "airbnb",
      "plugin:jsx-a11y/recommended"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "src"
          ]
        }
      }
    },
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {
      "linebreak-style": [
        "error",
        "windows"
      ],
      "react/jsx-props-no-spreading": "off",
      "no-confusing-arrow": "off",
      "no-debugger": "warn",
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      "last 1 edge version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1"
  }
}
